{
  "expo": {
    "name": "Obd",
    "slug": "Obd",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vwadaptation",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "Ši programėlė naudoja Bluetooth ryšį prisijungimui prie OBD-II adapterio.",
        "NSBluetoothPeripheralUsageDescription": "Ši programėlė naudoja Bluetooth ryšį prisijungimui prie OBD-II adapterio."
      }
    },
    "android": {
      "package": "com.vwadaptation.app",
      "permissions": [
        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "BLUETOOTH_CONNECT",
        "BLUETOOTH_SCAN",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "react-native-ble-plx",
        {
          "isBackgroundEnabled": true,
          "modes": ["peripheral", "central"],
          "bluetoothAlwaysPermission": "Ši programėlė naudoja Bluetooth ryšį prisijungimui prie OBD-II adapterio."
        }
      ]
    ],
        "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "dc494132-810b-40c3-b384-99978b9239e0"
      }
    }
  }
}
